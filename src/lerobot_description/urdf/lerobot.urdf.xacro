            <origin xyz="0.00025 0.006978 -0.065594" rpy="0 0 0"/>
            <geometry>
            <mesh filename="package://lerobot_description/meshes/limb1_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="limb2_1">
        <inertial>
            <origin xyz="0.018208420998056387 0.07233063631375242 0.002577881218926492" rpy="0 0 0"/>
