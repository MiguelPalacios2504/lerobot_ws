<?xml version="1.0" ?>
<robot name="lerobot" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:arg name="is_sim" default="true"/>

    <xacro:arg name="is_ignition" default="true"/>
    <xacro:include filename="$(find lerobot_description)/urdf/lerobot_gazebo.xacro"/>
    <xacro:include filename="$(find lerobot_description)/urdf/lerobot_ros2_control.xacro"/>
    <xacro:include filename="materials.xacro"/>

    
    <xacro:property name="PI" value="3.14159"/>
    <xacro:property name="ZERO" value="0.0"/>
    <xacro:property name="effort" value="30.0"/> 
    <xacro:property name="velocity" value="10.0"/>  

    <link name="world"/>

    <link name="base_link">
        <inertial>
            <origin xyz="-9.642459078465404e-05 0.019891856733790833 0.030552217737414043" rpy="0 0 0"/>
            <mass value="0.1914817969859309"/>
            <inertia ixx="0.000187" iyy="0.000189" izz="0.000196" ixy="0.0" iyz="-1.4e-05" ixz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://lerobot_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://lerobot_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

