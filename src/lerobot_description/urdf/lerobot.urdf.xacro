    <link name="limb5_1">
        <inertial>
            <origin xyz="0.0003179786709944605 -0.026780755665705133 -0.005169496291362458" rpy="0 0 0"/>
            <mass value="0.713822465631267"/>
            <inertia ixx="0.00047" iyy="0.000309" izz="0.000331" ixy="-1e-06" iyz="-7.3e-05" ixz="-4e-06"/>
        </inertial>
        <visual>
            <origin xyz="0.00205 0.121539 -0.153617" rpy="0 0 0"/>
            <geometry>
