    <joint name="base_joint" type="fixed">
        <parent link="world"/>
        <child link="base_link"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <joint name="limb1_to_base_link" type="revolute">
        <parent link="base_link"/>
        <child link="limb1_1"/>
        <axis xyz="-0.0 0.0 1.0"/>
        <origin xyz="-0.00025 -0.006978 0.065594" rpy="0 0 0"/>
        <limit lower="${-125*PI/180}" upper="${125*PI/180}" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="limb2_to_limb1" type="revolute">
        <origin xyz="-0.0189 -0.031273 0.05415" rpy="3.14159 0 0"/>
        <parent link="limb1_1"/>
        <child link="limb2_1"/>
        <axis xyz="-1.0 -0.0 -0.0"/>
        <limit lower="${-35*PI/180}" upper="${200*PI/180}" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="limb3_to_limb2" type="revolute">
        <origin xyz="-0.0006 0.112809 0.026967" rpy="3.14159 0 0"/>
        <parent link="limb2_1"/>
        <child link="limb3_1"/>
        <axis xyz="-1.0 -0.0 -0.0"/>
        <limit lower="${-160*PI/180}" upper="${20*PI/180}" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="limb4_to_limb3" type="revolute">
        <origin xyz="-0.0004 -0.134799 0.006374" rpy="0 0 0"/>
        <parent link="limb3_1"/>
        <child link="limb4_1"/>
        <axis xyz="-1.0 -0.0 0.0"/>
        <limit lower="${-PI/2}" upper="${PI/2}" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="limb5_to_limb4" type="revolute">
        <origin xyz="0.0181 -0.061298 0.000532" rpy="0 0 0"/>
        <parent link="limb4_1"/>
        <child link="limb5_1"/>
        <axis xyz="0.0 -0.999962 0.008688"/>
        <limit lower="${-150*PI/180}" upper="${150*PI/180}" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="limb6_to_limb5" type="revolute">
        <origin xyz="0.019 -0.02322 0.020865" rpy="1.57 0 0"/>
        <parent link="limb5_1"/>
        <child link="limb6_1"/>
        <axis xyz="1.0 0.0 0.0"/>
        <limit lower="${-100*PI/180}" upper="${10*PI/180}" effort="${effort}" velocity="${velocity}"/>
    </joint>

</robot>