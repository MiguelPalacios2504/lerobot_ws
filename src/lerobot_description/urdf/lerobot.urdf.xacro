    <link name="limb3_1">
        <inertial>
            <origin xyz="0.017969532376485707 -0.07703470120091216 0.0021380981205127436" rpy="0 0 0"/>
            <mass value="1.1077317609456931"/>
            <inertia ixx="0.001756" iyy="0.000395" izz="0.002003" ixy="1.3e-05" iyz="8.2e-05" ixz="-0.0"/>
        </inertial>
        <visual>
            <origin xyz="0.01975 -0.074558 -0.146711" rpy="0 0 0"/>
            <geometry>
            <mesh filename="package://lerobot_description/meshes/limb3_1.stl" scale="0.001 0.001 0.001"/>
