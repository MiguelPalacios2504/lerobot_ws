        <collision>
            <origin xyz="0.01975 -0.074558 -0.146711" rpy="0 0 0"/>
            <geometry>
            <mesh filename="package://lerobot_description/meshes/limb3_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="limb4_1">
        <inertial>
            <origin xyz="0.025371298583359233 -0.03515486574341366 0.0004912597019903209" rpy="0 0 0"/>
            <mass value="0.5194224938499433"/>
            <inertia ixx="0.000179" iyy="0.000204" izz="0.00031" ixy="3.6e-05" iyz="1e-06" ixz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="0.02015 0.060241 -0.153085" rpy="0 0 0"/>
