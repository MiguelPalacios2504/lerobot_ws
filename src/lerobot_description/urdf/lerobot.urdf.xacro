            <inertia ixx="0.000125" iyy="0.000123" izz="3.4e-05" ixy="0.0" iyz="4e-06" ixz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="0.00025 0.006978 -0.065594" rpy="0 0 0"/>
            <geometry>
            <mesh filename="package://lerobot_description/meshes/limb1_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
